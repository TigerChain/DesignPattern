<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_lazy_style_single"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="懒汉式单例"/>

    <Button
        android:id="@+id/btn_lazy_style_single_mutithread"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="多线程操作非线程安全的懒汉式单例"/>

    <Button
        android:id="@+id/btn_safe_lazystyle_mutithread"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="多线程操作线程安全的懒汉式单例"/>
    <Button
        android:id="@+id/btn_hungryman_singleton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="饿汉式单例"/>

    <Button
        android:id="@+id/btn_hungryman_mutithread"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="多线程操作饿汉式单例"/>

    <Button
        android:id="@+id/btn_enum_singleton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="枚举类单例"/>


</LinearLayout>
